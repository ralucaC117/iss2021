<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="242.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignInController">
   <children>
      <Button fx:id="buttonLogInUser" layoutX="97.0" layoutY="180.0" mnemonicParsing="false" onAction="#handleButtonLogInUser" text="Log in" />
      <PasswordField fx:id="textBoxPassword" layoutX="107.0" layoutY="130.0" promptText="password" />
      <TextField fx:id="textBoxCnp" layoutX="107.0" layoutY="92.0" promptText="cnp" />
      <Text fill="#716a6a" layoutX="52.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sistem de gestionare al bibliotecii" textAlignment="CENTER">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Text>
      <Button fx:id="buttonLogInOperator" layoutX="158.0" layoutY="180.0" mnemonicParsing="false" onAction="#handleButtonLogInOperator" text="Log in as operator" />
   </children>
</AnchorPane>
